{
  "name": "rank-stratos-seo-analyzer",
  "version": "1.0.0",
  "description": "A professional SEO analysis website that provides comprehensive website analysis using multiple SEO metrics",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "python": "cd backend && python app.py",
    "python-dev": "cd backend && python -m flask run --host=0.0.0.0 --port=5001",
    "install-python": "cd backend && pip install -r requirements.txt",
    "serve": "python3 -m http.server 3000",
    "serve-8080": "python3 -m http.server 8080",
    "serve-5000": "python3 -m http.server 5000",
    "serve-9000": "python3 -m http.server 9000",
    "build": "echo 'No build process needed'",
    "test": "echo 'No tests specified'",
    "open": "open http://localhost:3000"
  },
  "keywords": [
    "seo",
    "analysis",
    "website",
